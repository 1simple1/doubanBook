<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.simplem.doubanbook.mapper.ImageMapper">

    <select id="selectByBookId" parameterType="int" resultType="image">

    </select>

    <insert id="insert" parameterType="image">
        insert into image (bookId, imgUrl)
        values (#{bookId},#{imgUrl})
    </insert>
</mapper>